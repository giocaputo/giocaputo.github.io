<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">

  {% for page in site.pages %}
    {% if page.sitemap != false and page.url contains '.css' == false %}
    <url>
      <loc>{{ site.url }}{{ page.url }}</loc>
    </url>
    {% endif %}
  {% endfor %}

  <!-- Custom url -->
  <url>
    <loc>https://giocaputo.github.io/Vince-Chi-smette/sitemap.xml</loc>
  </url>

</urlset>
